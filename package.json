{
  "name": "tools",
  "main": "./lib/tools",
  "version": "0.0.1",
  "description": "yuanliwei (de) tool box!",
  "keywords": ["tools"],
  "activationCommands": {
    "atom-workspace": [
      "tools:run",
      "tools:toggle",
      "tools:JS format",
      "tools:CSS format",
      "tools:SQL format",
      "tools:XML format",
      "tools:JSON format",
      "tools:SQL min",
      "tools:XML min",
      "tools:CSS min",
      "tools:JSON min",
      "tools:Crypto md5",
      "tools:Crypto sha1",
      "tools:Crypto sha256",
      "tools:Crypto sha512",
      "tools:Encode encodeUri",
      "tools:Encode decodeUri"
    ]
  },
  "repository": "https://github.com/atom/tools",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "highlight.js": "^9.12.0",
    "iconv-lite": "^0.4.19",
    "jquery": "^3.3.1",
    "js-beautify": "^1.7.5",
    "jszip": "^3.1.5",
    "moment": "^2.21.0",
    "pako": "^1.0.6",
    "sql.js": "^0.5.0"
  }
}
